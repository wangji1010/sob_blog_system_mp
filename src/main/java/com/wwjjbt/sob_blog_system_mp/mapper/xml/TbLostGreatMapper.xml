<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wwjjbt.sob_blog_system_mp.mapper.TbLostGreatMapper">

    <select id="selectGreatBya_u" resultType="tbLostGreat">
        select * from tb_lost_great where u_id=#{u_id} and p_id=#{a_id}
    </select>

    <delete id="deleteGreatPoint">
        delete from tb_lost_great where id = #{id}
    </delete>

</mapper>
